{
  "name": "use-scraper",
  "version": "1.1.0",
  "description": "React hook that allow you to scrape data from a website",
  "keywords": [
    "scraper",
    "react",
    "hook",
    "useScraper"
  ],
  "homepage": "https://use-scraper.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alejandroid17/useScraper.git"
  },
  "license": "MIT",
  "author": "Alejandroid17",
  "bugs": {
    "url": "https://github.com/Alejandroid17/useScraper/issues"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "vite:build": "vite build && npm run cp:readme",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "storybook": "start-storybook -p 3000",
    "storybook:build": "build-storybook -c .storybook -o public",
    "cp:readme": "cp README.md dist/README.md",
    "eslint": "eslint './**/*.{js,jsx}'"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/react": "6.4.22",
    "@testing-library/react-hooks": "8.0.0",
    "@vitejs/plugin-react": "1.3.1",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "autoprefixer": "10.4.5",
    "babel-loader": "8.2.5",
    "concurrently": "7.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.29.4",
    "fetch-mock": "9.11.0",
    "jsdom": "19.0.0",
    "postcss": "8.4.13",
    "postcss-cli": "9.1.0",
    "postcss-import": "14.1.0",
    "postcss-nesting": "10.1.4",
    "prettier": "2.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "storybook-builder-vite": "0.1.23",
    "tailwindcss": "3.0.24",
    "vite": "2.9.6",
    "vitest": "0.10.0"
  } 
}
